mixin device(devices)
    ul.device-items.clearfix
        -each dev in devices
            li.device-items-item.clearfix
                .device.clearfix
                    ul.device-image.clearfix
                        li.device-image-item.device-image-item_main
                        li.device-image-item.device-image-item_1
                        li.device-image-item.device-image-item_2
                        li.device-image-item.device-image-item_3
                    ul.device-characters.clearfix
                        li.device-characters-item.clearfix= dev.name
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |Тип SIM-карты
                            span.device-characters-item-text_right.clearfix
                                = dev.sim
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |Кол-во SIM-карт
                            span.device-characters-item-text_right.clearfix
                                = dev.num_sim
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |Цвет
                            span.device-characters-item-text_right.clearfix
                                = dev.color
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |OC
                            span.device-characters-item-text_right.clearfix
                                = dev.os
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |Связь 3G
                            span.device-characters-item-text_right.clearfix
                                = dev.g_3
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |4G(LTE)
                            span.device-characters-item-text_right.clearfix
                                = dev.g_4
                        li.device-characters-item.clearfix
                            span.device-characters-item-text_left.clearfix
                                |GPS-модуль
                            span.device-characters-item-text_right.clearfix
                                = dev.gps
                    .device-rating-wrapper.clearfix
                        ul.device-rating.clearfix
                            li.rating-star
                            li.rating-star
                            li.rating-star
                            li.rating-star
                            li.rating-star.rating-star_off
                        .device-rating_number.clearfix 4
                    .device-price= dev.price
                    .device-cart.clearfix
                        .device-cart-status.clearfix В наличии
                        button.device-cart-push.clearfix В корзину
