mixin device(devices)
    -each dev in devices
        .device
            ul.device-image
                li.device-image_main
                    img(alt='main')
                li.device-image
                    img(alt='1')
                li.device-image
                    img(alt='2')
                li.device-image
                    img(alt='3')
            ul= dev.name
                li
                    |Тип SIM-карты
                    = dev.sim
                li
                    |Кол-во SIM-карт
                    = dev.num_sim
                li
                    |Цвет
                    = dev.color
                li
                    |OC
                    = dev.os
                li
                    |Связь 3G
                    = dev.g_3
                li
                    |4G(LTE)
                    = dev.g_4
                li
                    |GPS-модуль
                    = dev.gps
            ul.rating
                li.rating-star
                li.rating-star
                li.rating-star
                li.rating-star
                li.rating-star
            .price= dev.price
            .cart
                .cart-status В наличии
                button.cart-push В корзину
