include _mixins/_navigation.jade
include _mixins/_devices.jade
html(lang='ru-RU')
    include _common/_head.jade
    body
        .main-wrapper
            header.header
                .header-wrapperico
                    +social_ico(social_items)
                .header-wrapperadress
                    +navigation(adress_items)
            nav.navigation-head
                .navigation-head-logo
                    img(alt='logo')
                .navigation-head-search
                    input(type='text', placeholder='Что будем искать?')
                    button Искать
                +navigation(header_items)
            nav.navigation-menu
                +navigation(items)
            .central-wrapper
                .sidebar
                    .sidebar-cat
                        +navigation(category)
                    .sidebar-accordion
                        ul
                            +accardeon(sidebar.first)
                            +accardeon(sidebar.second)
                            ul Цены
                                lable
                                    |От
                                    input.from(type="text")
                                lable
                                    |До
                                    input.from(type="text")
                            ul Цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                                li цвет
                            +accardeon(sidebar.third)
                .content
                    .content-view
                        .content-view_left
                            lable
                                |Сортировать по цене
                                select
                                    option по цене
                                    option по названию
                        .content-view_right
                            ul Вид каталога
                                li 1
                                li 2
                                li 3
                    .content-items
                        +device(devices)
            .information
